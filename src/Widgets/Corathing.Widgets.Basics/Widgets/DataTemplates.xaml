<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fileopener="clr-namespace:Corathing.Widgets.Basics.Widgets.FileOpeners"
                    xmlns:commanders="clr-namespace:Corathing.Widgets.Basics.Widgets.Commanders"
                    xmlns:linkers="clr-namespace:Corathing.Widgets.Basics.Widgets.LinkOpeners"
                    xmlns:monacos="clr-namespace:Corathing.Widgets.Basics.Widgets.Monacos"
                    xmlns:notes="clr-namespace:Corathing.Widgets.Basics.Widgets.Notes"
                    xmlns:timers="clr-namespace:Corathing.Widgets.Basics.Widgets.Timers"
                    xmlns:todolists="clr-namespace:Corathing.Widgets.Basics.Widgets.ToDoLists"
                    xmlns:webpages="clr-namespace:Corathing.Widgets.Basics.Widgets.WebPages"
                    xmlns:webqueries="clr-namespace:Corathing.Widgets.Basics.Widgets.WebQueries">
    
    <DataTemplate DataType="{x:Type fileopener:FileOpenerWidgetViewModel}">
        <fileopener:FileOpenerWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type commanders:CommanderWidgetViewModel}">
        <commanders:CommanderWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type linkers:LinkOpenerWidgetViewModel}">
        <linkers:LinkOpenerWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type monacos:MonacoWidgetViewModel}">
        <monacos:MonacoWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type notes:NoteWidgetViewModel}">
        <notes:NoteWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type timers:TimerWidgetViewModel}">
        <timers:TimerWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type todolists:ToDoListViewModel}">
        <todolists:ToDoListWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type webpages:WebPageViewModel}">
        <webpages:WebPageWidget/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type webqueries:WebQueryViewModel}">
        <webqueries:WebQueryWidget />
    </DataTemplate>
    
</ResourceDictionary>